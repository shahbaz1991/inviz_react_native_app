      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.shahbaza7.invizdemoapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
