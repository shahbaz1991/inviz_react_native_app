{
  "expo": {
    "name": "NewExpoProject",
    "slug": "inviz-demo-app",
    "owner": "shahbaza7",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
